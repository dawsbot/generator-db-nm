{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %>
    "dawson"
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0"
  <% } %>},
  "devDependencies": {
    "ava": "^0.11.0",
    "xo": "^0.12.1",
    "eslint-config-xo-space": "^0.8.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "eslintConfig": {
      "extends": "xo-space"
    }
  }
}
