{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "homepage": "https://github.com/<%= githubUsername %>/<%= moduleName %>#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUsername %>/<%= moduleName %>.git"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= moduleName %>/issues"
  },<% if (cli) { %>
  "bin": {
    "<%= moduleName %>": "cli.js"
  },<% } %>
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "xo && ava"<% if (bundle) { %>,
    "bundle": "browserify index.js -s <% camelModuleName %> -t [ babelify --presets [ es2015 ] ] | uglifyjs > bundle.js",
    "precommit": "npm test && npm run bundle "<% } %>
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } if (bundle) { %>,
    "bundle.js <% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %><% if (appveyor) { %>
    "windows",<% } %><% if (bundle) { %>
    "bundled",
    "web-ready",<% } %>
    "documented",
    "tested",
    "licensed",
    "dawson"
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0",
    "update-notifier": "^1.0.2"
  <% } %>},
  "devDependencies": {
    "ava": "^0.16.0",
    "xo": "^0.16.0"<% if (bundle) { %>,
    "uglify-js": "^2.6.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0"<% } %>
  },
  "xo": {
    "space": true
  }
}
