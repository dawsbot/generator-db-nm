{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "homepage": "https://github.com/<%= githubUsername %>/<%= moduleName %>#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUsername %>/<%= moduleName %>.git"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= moduleName %>/issues"
  },<% if (cli) { %>
  "bin": {
    "<%= moduleName %>": "cli.js"
  },<% } %>
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "xo && ava",
    "ava": "ava"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %><% if (appveyor) { %>
    "windows",<% } %>
    "lean",
    "minimal",
    "documented",
    "tested",
    "licensed",
    "ava",
    "dawson"
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0",
    "update-notifier": "^0.6.1"
  <% } %>},
  "devDependencies": {
    "ava": "*.*.*",
    "xo": "*.*.*",
    "eslint-config-xo-space": "^0.8.0"
  },
  "xo": {
    "space": true,
    "eslintConfig": {
      "extends": "xo-space"
    }
  }
}
