{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "homepage": "https://github.com/<%= githubUsername %>/<%= moduleName %>#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUsername %>/<%= moduleName %>.git"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= moduleName %>/issues"
  },<% if (cli) { %>
  "bin": {
    "<%= moduleName %>": "cli.js"
  },<% } %>
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "xo && ava",<% if (bundle) { %>
    "bundle": "browserify index.js -s <% camelModuleName %> -o bundle.js -t [ babelify --presets [ es2015 ] ]",<% } %>
    "precommit": "npm test<% if (bundle) { %> && npm run bundle <% } %>"
  },
  "files": [
    "index.js"<% if (cli) { %>,
    "cli.js"<% } %>
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %><% if (appveyor) { %>
    "windows",<% } %><% if (bundle) { %>
    "bundled",
    "web-ready",<% } %>
    "lean",
    "minimal",
    "documented",
    "tested",
    "licensed",
    "ava",
    "dawson"
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0",
    "update-notifier": "^0.6.1"
  <% } %>},
  "devDependencies": {
    "ava": "^0.13.0",
    "xo": "^0.12.1",<% if (bundle) { %>
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint-config-xo-space": "^0.8.0",<% } %>
    "husky": "^0.11.3",
    "eslint-config-xo-space": "^0.8.0"
  },
  "xo": {
    "space": true,
    "eslintConfig": {
      "extends": "xo-space"
    }
  }
}
