{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "homepage": "https://github.com/<%= githubUsername %>/<%= moduleName %>#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUsername %>/<%= moduleName %>.git"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= humanizedWebsite %>"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUsername %>/<%= moduleName %>/issues"
  },<% if (cli) { %>
  "bin": {
    "<%= moduleName %>": "dist/cli.js"
  },<% } %>
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "xo && npm run prepublish && nyc ava",
    "prepublish": "babel lib --out-dir=dist",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
    "test": "xo && ava"<% if (web) { %>,
    "browser": "webpack --config config/webpack.conf.js",
    "bundle": "browserify index.js -s <% camelModuleName %> -t [ babelify --presets [ es2015 ] ] > bundle.js"<% } %>
  },
  "main": [
    "dist/index.js"
  ],
  "files": [
    "dist"
  ],
  "keywords": [<% if (cli) { %>
    "cli-app",
    "cli",<% } %><% if (appveyor) { %>
    "windows",<% } %><% if (web) { %>
    "bundled",
    "web-ready",<% } %>
    "lean",
    "documented",
    "tested",
    "licensed",
    "dawson"
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.7.0",
    "update-notifier": "^1.0.2"
  <% } %>},
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "coveralls": "^2.11.11",
    "nyc": "^7.0.0",
    "xo": "^0.16.0"<% if (web) { %>,
    "webpack": "^1.12.14",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4"
    <% } %>
  },
  "xo": {
    "space": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
